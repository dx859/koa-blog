{
  "apps" : [
    {
      "name": "blog-dev",
      "script": "bin/www",
      "watch" : true,
      "error_file": "log/err-dev.log",
      "out_file": "log/out-dev.log",
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "env": {
        "NODE_ENV": "development"
      }
    },
    {
      "name": "blog",
      "script": "bin/www",
      "watch" : false,
      "error_file": "log/err.log",
      "out_file": "log/out.log",
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "env": {
        "NODE_ENV": "production"
      }
    }
  ]
}